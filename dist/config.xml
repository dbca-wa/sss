<?xml version='1.0' encoding='utf-8'?>
<widget id="io.dpaw.sss" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>gokart</name>
    <description>
        A mobile map application
    </description>
    <author email="OIMSupport@dpaw.wa.gov.au" href="https://oim.dpaw.wa.gov.au">
        OIM Support Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="CodePushDeploymentKey" value="GkSj_Q-53gALdSLSpEAqUEgUcSSj4kPEOeyvb" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="CodePushDeploymentKey" value="GkSj_Q-53gALdSLSpEAqUEgUcSSj4kPEOeyvb" />
    </platform>
    <preference name="LogLevel" value="VERBOSE" />
    <preference name="xwalkVersion" value="19+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <engine name="android" spec="~5.2.1" />
    <engine name="ios" spec="~4.2.1" />
</widget>
